{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "sourceMap": true,
    "outDir": "./bundle",
    "strict": true,
    "baseUrl": "./src",
    "rootDirs": [
      "./"
    ],
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "node_modules",
    "tempAsset",
    "tempAsset/*.*",
    "access*.*",
    "error*.*"
  ]
}